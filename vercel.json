{
  "version": 2,
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/client/public/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/index.ts"
    }
  ]
}

